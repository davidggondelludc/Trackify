<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_playlist">

    <fragment
        android:id="@+id/navigation_playlist"
        android:name="com.apm.trackify.ui.playlist.PlaylistFragment"
        tools:layout="@layout/playlists_landing_fragment" />

    <fragment
        android:id="@+id/navigation_routes"
        android:name="com.apm.trackify.ui.routes.RoutesFragment"
        tools:layout="@layout/routes_search_fragment" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.apm.trackify.ui.profile.ProfileFragment"
        tools:layout="@layout/user_profile_fragment" />
